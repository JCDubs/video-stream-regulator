        "json-schema-to-ts": "^1.5.0",
        "pre-commit": "^1.2.2",
        "serverless": "^2.23.0",
        "serverless-dynamodb-local": "^0.2.39",
        "serverless-offline": "^6.9.0",
        "serverless-webpack": "^5.3.5",
        "ts-jest": "^26.5.4",
        "ts-loader": "^8.0.15",
        "ts-node": "^9.1.1",
        "tsconfig-paths": "^3.9.0",
        "tsconfig-paths-webpack-plugin": "^3.3.0",
        "typescript": "^4.1.3",
        "webpack": "^5.20.2",
        "webpack-node-externals": "^2.5.2"
